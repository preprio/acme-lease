{
    "extends": ["next/core-web-vitals", "next/typescript", "prettier"],
    "rules": {
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "^_",
                "varsIgnorePattern": "^_"
            }
        ],
        "prefer-const": "error",
        "@typescript-eslint/no-explicit-any": "warn"
    }
}
